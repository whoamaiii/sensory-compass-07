# Sensory Compass PoC Environment Variables
# Copy this to .env.local for local development

# App Configuration
VITE_APP_NAME="Sensory Compass"
VITE_APP_VERSION="0.1.0-poc"
VITE_APP_ENV="development"

# Feature Flags (for PoC demo control)
VITE_FEATURE_AI_INSIGHTS=true
VITE_FEATURE_EXPORT_REPORTS=true
VITE_FEATURE_MOCK_DATA=true
VITE_FEATURE_DEMO_MODE=true

# Demo Configuration
VITE_DEMO_AUTO_POPULATE=true
VITE_DEMO_STUDENT_COUNT=3
VITE_DEMO_DATA_POINTS=50

# Analytics (optional for PoC)
VITE_ANALYTICS_ENABLED=false
# VITE_GA_TRACKING_ID=

# API Configuration (if needed)
VITE_API_URL="https://api.example.com"
VITE_API_MOCK=true

# Storage
VITE_USE_LOCAL_STORAGE=true
VITE_STORAGE_PREFIX="sensory_compass_poc_"
